CONFIG_PATH=/etc/core/app.conf
UAA_CA_ROOT=/etc/core/certificates/uaa_ca.pem
_REDIS_URL_CORE=redis://{{ harbor_cache_cont_name }}:6379?idle_timeout_seconds=30
SYNC_QUOTA=true
CHART_CACHE_DRIVER=redis
_REDIS_URL_REG=redis://{{ harbor_cache_cont_name }}:6379/1?idle_timeout_seconds=30

LOG_LEVEL=debug
EXT_ENDPOINT=http://{{ harbor_domain }}
DATABASE_TYPE=postgresql
POSTGRESQL_HOST=postgresql
POSTGRESQL_PORT=5432
POSTGRESQL_USERNAME={{ harbor_db_username | mandatory }}
POSTGRESQL_PASSWORD={{ harbor_db_password | mandatory }}
POSTGRESQL_DATABASE={{ harbor_db_database}}
POSTGRESQL_SSLMODE=disable
POSTGRESQL_MAX_IDLE_CONNS=100
POSTGRESQL_MAX_OPEN_CONNS=900
POSTGRESQL_CONN_MAX_LIFETIME=5m
POSTGRESQL_CONN_MAX_IDLE_TIME=0
REGISTRY_URL=http://{{ harbor_registry_cont_name }}:5000
PORTAL_URL=http://{{ harbor_portal_cont_name }}:8080
TOKEN_SERVICE_URL=http://{{ harbor_core_cont_name }}:8080/service/token
HARBOR_ADMIN_PASSWORD={{ harbor_admin_password | mandatory }}
MAX_JOB_WORKERS=10
CORE_SECRET={{ harbor_core_secret | mandatory}}
JOBSERVICE_SECRET={{ harbor_job_service_secret | mandatory }}
WITH_NOTARY=False
WITH_TRIVY=False
CORE_URL=http://{{ harbor_core_cont_name }}:8080
CORE_LOCAL_URL=http://127.0.0.1:8080
JOBSERVICE_URL=http://{{harbor_job_service_cont_name }}:8080
TRIVY_ADAPTER_URL=http://trivy-adapter:8080
NOTARY_URL=http://notary-server:4443
REGISTRY_STORAGE_PROVIDER_NAME=filesystem
READ_ONLY=false
RELOAD_KEY=
CHART_REPOSITORY_URL=http://chartmuseum:9999
REGISTRY_CONTROLLER_URL=http://{{ harbor_registryctl_cont_name }}:8080
WITH_CHARTMUSEUM=False
REGISTRY_CREDENTIAL_USERNAME={{ harbor_registry_username | mandatory }}
REGISTRY_CREDENTIAL_PASSWORD={{ harbor_registry_password | mandatory }}
CSRF_KEY={{ harbor_csrf_key |mandatory }}
PERMITTED_REGISTRY_TYPES_FOR_PROXY_CACHE=docker-hub,harbor,azure-acr,aws-ecr,google-gcr,quay,docker-registry,github-ghcr,jfrog-artifactory

HTTP_PROXY=
HTTPS_PROXY=
NO_PROXY=registry,notary-server,core,chartmuseum,postgresql,trivy-adapter,exporter,registryctl,log,127.0.0.1,.local,nginx,db,jobservice,.internal,redis,notary-signer,localhost,portal

PORT=8080

METRIC_ENABLE=true
METRIC_PATH=/metrics
METRIC_PORT=9090
METRIC_NAMESPACE=harbor
METRIC_SUBSYSTEM=core
